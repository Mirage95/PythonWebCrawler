def median(a,b,c):
    if (a >= b and a <= c) or (a <= b and a >= c):
        return a
    if (b >= a and b <= c) or (b <= a and b >= c):
        return b
    if (c >= a and c <= b) or (c <= a and c >= b):
        return c
    
print(median(1,2,3))
#>>> 2

print(median(9,3,6))
#>>> 6

print(median(7,8,7))
#>>> 7
